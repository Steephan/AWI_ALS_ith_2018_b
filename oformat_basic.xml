<?xml version="1.0" encoding="ISO-8859-1" ?>

<!-- Exemplary ASCII format definition that imports/exports point coordinates, followed by amplitudes -->

<opalsFormatDefinition>
  <ascii>
    <columnSeparators val=" "/>
    <data>        
      <entry val="x"/>       
      <entry val="y"/>       
      <entry val="z"/>
      <entry val="Amplitude"/>
      <entry val="EchoWidth"/>
	  <entry val="EchoNumber"/>
	  <entry val="_EchoCount" externalType="int"/>
      <entry val="_EchoRank" externalType="int"/>
      <entry val="FileId" externalType="int"/>
    </data>
  </ascii>
</opalsFormatDefinition>
