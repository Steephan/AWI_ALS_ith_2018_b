<?xml version="1.0" encoding="ISO-8859-1" ?>

<!-- Exemplary ASCII format definition that imports/exports point coordinates, followed by amplitudes -->

<opalsFormatDefinition>
  <ascii>
    <columnSeparators val="	"/>
    <header>
      <text val="X[m]	Y[m]	Z[m]	Amplitude[DN]	EchoWidth[ns]	EchoType[DN]	TerrainProbability[DecimalFraction]	RelativeHeight[m]	Class[DN]	PointSourceId[DN]" />
    </header>
    <data>        
      <entry val="x"/>       
      <entry val="y" />       
      <entry val="z"/>
      <entry val="Amplitude"/>
      <entry val="EchoWidth"/>
      <entry val="_EchoType" externalType="int"/>
	  <entry val="_terrainProb" externalType="float"/>
	  <entry val="_nZ_corr" externalType="float"/>
	  <entry val="Classification"/>
	  <entry val="PointSourceId"/>
    </data>
  </ascii>
</opalsFormatDefinition>
